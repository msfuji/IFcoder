{
  "name": "ifcoder-dev",
  "build": { "dockerfile": "../Dockerfile" },
  "workspaceFolder": "/work",
  "workspaceMount": "source=${localWorkspaceFolder},target=/work,type=bind",
  "remoteEnv": { "PYTHONPATH": "/work/src" },
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-python.vscode-pylance"
      ]
    }
  }
}

